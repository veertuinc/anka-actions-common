{
  "name": "anka-actions-common",
  "version": "1.0.0",
  "private": false,
  "description": "Anka actions common lib",
  "main": "lib/main.js",
  "types": "lib/main.d.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "test": "jest --passWithNoTests",
    "all": "npm run build && npm run format && npm run lint && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/veertuinc/anka-actions-common.git"
  },
  "keywords": [
    "anka",
    "actions"
  ],
  "author": "Veertu Inc.",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@octokit/core": "^4.1.0",
    "@octokit/rest": "^19.0.5",
    "axios": "^1.1.3"
  },
  "devDependencies": {
    "@types/node": "^18.11.6",
    "@typescript-eslint/parser": "^5.41.0",
    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.26.0",
    "eslint-plugin-github": "^4.4.0",
    "eslint-plugin-jest": "^27.1.3",
    "jest": "^29.2.2",
    "js-yaml": "^4.1.0",
    "prettier": "2.7.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  }
